{
  "manifest_version": 3,
  "name": "Good-or-Mood",
  "version": "1.0",
  "description": "What's your mood today?",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["content.js"]
    }
  ]
}
